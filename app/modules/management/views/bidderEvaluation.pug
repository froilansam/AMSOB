		script.
			$(document).ready(function(){
				$('#table').dataTable();
				$(document).on('click', '.credentials', function(){
					$('#idPicture').attr('src', `/assets/uploads/${$(this).attr('idPicture')}`)
					$('#idType').val(`${$(this).attr('idType')}`)
					$('#idNumber').val(`${$(this).attr('idNumber')}`)
					$('#accept').attr('bidderId', `${$(this).attr('bidderId')}`)
					$('#decline').attr('bidderId', `${$(this).attr('bidderId')}`)
					$('#credentialsModal').modal('toggle')
				});
				$('#credentialsModal').on('hidden.bs.modal', function(){
					$('#append').empty()
				})
				$(document).on('click', '#accept', function(){
					var idNow = $(this).attr('bidderId')
					swal({
						title: 'Are you sure?',
						text: 'Do you really want to accept this bidder?',
						icon: 'info',
						buttons: {
							no: 'No',
							yes: 'Yes',
						}
					}).then(function(value){
						if(value == 'yes'){
							$.post('/evaluation/bidder',{id: idNow, request: 'accept'})
							.done(function(data){
								console.log(data)
								if(data.response == 'updated'){
									var table = $('#table').DataTable();
									table.clear();
									table.destroy()
									for(var i=0;i<data.refetch.length;i++){
										$('#tableBody').append(`<tr>
											<td><span class="dropdown">
												<button class="btn btn-info dropdown-toggle dropdown-menu-right" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
												<i class="ft-settings"></i>
												</button>
												<span class="dropdown-menu mt-1 dropdown-menu-right">
													<a class="credentials dropdown-item" href="javascript:void(0)" bidderId='${data.refetch[i].intBidderID}' idType='${data.refetch[i].strIDType}' idNumber='${data.refetch[i].strIDNumber}' idPicture='${data.refetch[i].strIDPicture}'>
														<i class="icon-eye"></i> Credentials
													</a>
												</span>
											</td>
											<td>${data.refetch[i].strName} </td>
											<td>${data.refetch[i].strContact}</td>
											<td>${data.refetch[i].strAddress}</td>
											<td>${data.refetch[i].strEmailAddress}</td>
										</tr>`)
									}
									$('#table').dataTable()
									$('#credentialsModal').modal('toggle');
									toastr.success("Yay, you have successfully evaluated a bidder!","Success!",{
										showMethod:"slideDown",
										hideMethod:"slideUp",
										positionClass:"toast-bottom-right",
										timeOut:5000
									})
								}
							})
						}
					})
				});
			});