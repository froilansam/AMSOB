		.app-content.content
			.content-wrapper
				.content-header.row
					.content-header-left.col-md-6.col-12.mb-2.breadcrumb-new
						h3.content-header-title.mb-0.d-inline-block Lost and Damage
						.row.breadcrumbs-top.d-inline-block
				.content-body
					.row
						.col-12
							.card
								.card-head
									.card-header
										h4.card-title Items with Issues
										a.heading-elements-toggle
											i.la.la-ellipsis-h.font-medium-3
										.heading-elements
								.card-content
									.card-body
										.table-responsive
											table#table.table.table-white-space.table-bordered.row-grouping.display.no-wrap.icheck.table-middle
												thead
													tr
														th Consignment ID
														th Item
														th Issue
														th Quantity
														th Date Recorded
														th Action
												tbody
													each issue in issues
														tr
															td #{issue.intCIConsignment}
															-issue.jsonOtherSpecifications = JSON.parse(issue.jsonOtherSpecifications);
															-if(issue.jsonOtherSpecifications.hasOwnProperty('Screensize'))
																-if(!issue.jsonOtherSpecifications.Screensize.indexOf('Screensize'))
																	-issue.jsonOtherSpecifications['Screensize'] = issue.jsonOtherSpecifications['Screensize']+' inches';						
															-if(issue.jsonOtherSpecifications.hasOwnProperty('Wattage'))
																-if(issue.jsonOtherSpecifications.Wattage.indexOf('watts') < 0)
																	-issue.jsonOtherSpecifications['Wattage'] = issue.jsonOtherSpecifications['Wattage']+' watts';
															-if(issue.jsonOtherSpecifications.hasOwnProperty('Storage') && issue.jsonOtherSpecifications.category == "Mobile")
																-if(!issue.jsonOtherSpecifications.Storage.indexOf('Storage'))
																	-issue.jsonOtherSpecifications['Storage'] = issue.jsonOtherSpecifications['Storage']+' GB';
															-if(issue.jsonOtherSpecifications.hasOwnProperty('Ram'))
																-if(!issue.jsonOtherSpecifications.Ram.indexOf('RAM'))
																	-issue.jsonOtherSpecifications['Ram'] = 'RAM '+issue.jsonOtherSpecifications['Ram']+' GB';
															-var valuesOnly = Object.values(issue.jsonOtherSpecifications)
															-var desc = issue.jsonOtherSpecifications.Item+' '
															-for(var l = 1; l < valuesOnly.length; l++)
																-desc = desc+' '+valuesOnly[l]
															td #{desc}
															td #{issue.strIssue}	
															td #{issue.issueQty}
															td #{issue.datDateSeen}
															td
																- if(issue.booItemStatus == 2)
																	span.btn.btn-sm.btn-outline-success.round Sold
																- else if(issue.booItemStatus == 4)
																	span.btn.btn-sm.btn-outline-secondary.round Pulled out
																- else
																	a.delete.btn.btn-danger.btn-sm(href='javascript:void(0)' issueId=`${issue.intIssuesID}`)
																		i.ft-trash-2
																		|  Delete
