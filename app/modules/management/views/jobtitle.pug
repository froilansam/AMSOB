		script.
			$(document).ready(function(){
				$('#table').dataTable();
				$('.checkie').iCheck({
					checkboxClass: 'icheckbox_flat-blue',
				});
				$(document).on('click', '#createButton', function(){
					swal({
						icon: 'info',
						title: 'Job Title',
						content:{
							element: 'input',
							attributes: {
								placeholder: '(e.g Manager, Accountant)',
								type: 'text'
							}
						}
					}).then(function(value){
						
						if(value!='' && value!=null){
							$.post('/jobtitle', {description: value})
							.done(function(data){
								var table = $('#table').DataTable();
								var row = $(`<tr>
									<td>${data.id}</td>
									<td>${data.desc}</td>
									<td><input class='acquisition checkie' type='checkbox' jobId='${data.id}'/></td>
									<td><input class='inventory checkie' type='checkbox' jobId='${data.id}'/></td>
									<td><input class='auction checkie' type='checkbox' jobId='${data.id}'/></td>
									<td><span class="dropdown">
											<button type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true" class="btn btn-info dropdown-toggle dropdown-menu-right"><i class="ft-settings"></i></button>
												<span class="dropdown-menu mt-1 dropdown-menu-right"><a href="javascript:void(0)" value="${data.id}" class="delete dropdown-item">
												<i class="ft-trash-2"></i> Delete</a><a href="javascript:void(0)" value="${data.id}" class="edit dropdown-item">
													<i class="icon-pencil"></i> Edit</a>
												</span>
										</span></td>
									</tr>`
								)
								table.row.add(row).draw();
								$('.checkie').iCheck({
									checkboxClass: 'icheckbox_flat-blue',
								});
								toastr.success("Yay, you have successfully added a Job Title","Success!",{
									showMethod:"slideDown",
									hideMethod:"slideUp",
									positionClass:"toast-bottom-right",
									timeOut:5000
								})
							})
						}
					})
				})
				
				$(document).on('click', '.edit', function(){
					var id = $(this).attr('value');
					$.post('/jobtitle/view', {id: $(this).attr('value')})
					.done(function(data){
						var job = data.jobtitle.strJobTitle;
							swal({
								icon: 'info',
								title: 'Job Title',
								content:{
									element: 'input',
									attributes: {
										value: job,
										type: 'text'
									}
								}
						}).then(function(value){
						if(value!='' && value!=null){
							$.post('/jobTitle/edit', {description: value, id})
							.done(function(data){
								var tableDelete = $('#table').DataTable();
								tableDelete.clear();
								for(var i=0; i<data.jobs.length;i++){
									var rowDelete = $(`<tr>
														<td>${data.jobs[i].intJobTitleID}</td>
														<td>${data.jobs[i].strJobTitle}</td>
														</tr>`
