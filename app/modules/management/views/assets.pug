		script.
			function objectifyForm(formArray) {//serialize data functio
				var returnArray = {};
				for (var i = 0; i < formArray.length; i++){
					returnArray[formArray[i]['name']] = formArray[i]['value'];
				}
				return returnArray;
			}
			function hideModal(){
				$('#addAssetModal2').on('hide.bs.modal', function(e){//- reset form everytime clicked outside.
					e.preventDefault();

					swal({
						title: 'You are about to close the asset form.',
						text: 'This action cannot be undone. Are you sure?',
						icon: 'warning',
						buttons: {
							Cancel: 'Cancel',
							Yes: 'Yes',
						}
					}).then((value)=>{
						if(value == 'Yes'){
							$('#item')
									.val('') 
									.parents('fieldset').nextAll().remove()
							$('#item').removeAttr('disabled', '')
							$('#YearReleased').val('')
							$('#OGprice').val('')
							$('#steps').val(0)
							$('#addAssetModal2').off('hide.bs.modal').modal('toggle')
							$('#add').fadeOut(1000, function(){
								$('#next').fadeIn(1000)
							});
							$('#prev').attr('disabled', '');
							hideModal();
						}
					})
					
				})
			}
