		script.
			$(window).on('load', function(){
				Chart.defaults.hitRateDoughnut=Chart.defaults.doughnut;
				t=Chart.controllers.doughnut.prototype.draw;
				var r=Chart.controllers.doughnut.extend({
					draw:function(){
						t.apply(this,arguments);
						var e=this.chart.chart.ctx,
							o=e.fill,
							a=this.chart.width,
							r=this.chart.height,
							i=(r/114).toFixed(2);
						this.chart.ctx.font=i+"em Verdana",
						this.chart.ctx.textBaseline="middle";
						var l=Math.round((a-this.chart.ctx.measureText("77%").width)/2),
							n=r/2;
						this.chart.ctx.fillText("77%",l,n),
						e.fill=function(){
							e.save(),
							e.shadowColor="#bbbbbb",
							e.shadowBlur=30,
							e.shadowOffsetX=0,
							e.shadowOffsetY=12,
							o.apply(this,arguments),
							e.restore()
						}
					}
				});
				Chart.controllers.hitRateDoughnut=r;
				o=document.getElementById("hit-rate-doughnut"),
				new Chart(o,{
					type:"hitRateDoughnut",
					data:{
						labels:["Remain","Completed"],
						datasets:[{
							label:"Favourite",
							backgroundColor:["#ffcece",	"#28D094"],
							data:[23,77]
						}]
					},
					options:{
						responsive:!0,
						title:{
							display:!1
						},
						legend:{
							display:!1
						},
						layout:{
							padding:{
								left:16,
								right:16,
								top:16,
								bottom:16
							}
						},
						cutoutPercentage:92,
						animation:{
							animateScale:!1,
							duration:2500
						}
					}
				});
			})
		script.
			$(document).ready(function(){
				$(".tooltip-track-mouse").tooltip({
					track:!1,
				})
				$(document).on('click', '#discardChanges', function(){
					$('form#infoForm').data('formValidation').destroy()
					var consignorID = !{JSON.stringify(consignorData.intConsignorID)}
					console.log(consignorID);
					$.post(`/consignor/data`,{
						intConsignorID: consignorID,
					}).done(function(data){
						if(data.indicator == 'success'){
							var consignorData = data.consignorData;
							$('#email').val(consignorData.strEmail);
							$('#username').val(consignorData.strUsername);
							if(consignorData.booConsignorType == '0'){
								$('#firstName').val(consignorData.strRepresentativeFirstName);
								$('#lastName').val(consignorData.strRepresentativeLastName);								
							}
							$('#address').val(consignorData.strAddress);
							if(consignorData.strPhone != null){
								$('#phone').val(consignorData.strPhone);
							}
							else if( consignorData.strPhone == null || consignorData.strPhone == ''){
								$('#phoneField').empty()
							}

							if(consignorData.strTelephone != null){
								$('#telephone').val(consignorData.strTelephone);
							}
							else if(consignorData.strTelephone == null){
								$('#telephoneField').empty()							
							}
							$('#companyName').remove()
							$('form#infoForm').find('input').attr('readonly', '');
							$('form#infoForm').find('textarea').attr('readonly', '');
						}
					})
					
					
					var consignorType = !{JSON.stringify(consignorData.booConsignorType)}					
					if(consignorType == 1)//- Personal No name
						$('#nameField').empty();
					$('#buttonField').fadeOut(1000, function(){
						var buttonReplace = $(`<button id="editInfo" class="btn btn-outline-info btn-min-width box-shadow-3 mr-1 mb-1"><i class="la la-edit"></i> Edit Information</button>`)
						$(this)
							.empty()
							.append( buttonReplace ).hide().fadeIn(1000);
					})					
				})
			})
		




			
