		//- Script Requirement Field 
		script.
			$(document).ready(function(){
				var proofCounter = 0;
				$('#personalID').val(!{JSON.stringify(consignorData.strIDType)})
				$('#personalID').select2({//- Select2 Initialization
					width: 'resolve',
				});

				$(document).on('click', '#editRequirement', function(){
					$('form#requirementField').find('input[readonly]').removeAttr('readonly');
					$('form#requirementField').find('select[disabled]').removeAttr('readonly');
					$('form#requirementField').find('select[disabled]').removeAttr('disabled');
					$('form#requirementField').find('input[disabled]').removeAttr('disabled');

					$(document).on('click', '.dropify-clear', function(){
						$('form#requirementField').formValidation('revalidateField', 'idProof');
						proofCounter = 1;
					})
					var consignorID = !{JSON.stringify(consignorData.intConsignorID)}
					$.post(`/consignor/data`,{
						intConsignorID: consignorID,
					}).done(function(data){
						if(data.indicator == 'success'){
							$('#proofField').empty().append($(`<div class="row">
													<div class="col-md-12">
														<input id="idProof" data-default-file="/assets/uploads/${data.consignorData.strIDPicture}" type="file" name="idProof" accept="image/*" data-height="300" data-allowed-file-extensions="jpeg jpg png" class="dropify"/>
													</div>
													</div>`)).hide().fadeIn(1000);
							$('#idProof').dropify({
								allowedFiles: ['png', 'jpg', 'jpeg', 'gif', 'bmp'],
								messages: {
									'default': 'Drag and drop an image here or click',
									'replace': 'Drag and drop or click to replace',
									'remove': 'Remove',
									'error': 'Ooops, you can only upload JPG, JPEG, and PNG files.'
								},
								error: {
									'fileSize': 'The file size is too big (1M max).',
									'fileFormat': 'The file format is not allowed ({{ value }} only).'
								}
							});

							$('form#requirementField').formValidation({
								framework: 'bootstrap',
								icon: {
									valid: 'fa fa-check',
									invalid: 'fa fa-times',
									validating: 'fa fa-spinner'
								},
								fields: {
									tinNumber: {
										validators: {
											notEmpty: {
												message: 'Enter your business TIN Number, this will be used for company validation.'
											},
										}
									},
									idNumber: {
										validators: {
											notEmpty: {
												message: 'Enter your your presented ID number.'
											},
										}
									},

									idProof: {
										validators: {
											notEmpty: {
												message: 'Please upload a photo of your ID for verification.'
											},
										}
									},
								}
							})
							.on('err.field.fv', function(e, data) {
								// Get the first invalid field
								var invalidFields = data.fv.getInvalidFields().length;
								$('#errorfieldRequirement').val(invalidFields)
							})
							.on('success.field.fv', function(e, data) {
								// Get the first invalid field
								var invalidFields = data.fv.getInvalidFields().length;
								$('#errorfieldRequirement').val(invalidFields)
							});//- FormValidation for requirments
						}
					})
					

					$('#editRequirementField').fadeOut(1000, function(){
